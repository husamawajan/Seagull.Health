@model DashboardModel
@using Seagull.Services.Security
@using Seagull.Services.Users
@functions{
    public void CheckSelectedTab(string tabName, int count)
    {
        if (ViewData["SelectedTab"] == null && count == 1)
        {
            ViewData["SelectedTab"] = tabName;
        }
    }
}
@{
    Html.AppendCssFileParts("~/Administration/Content/ionicons/css/ionicons-2.0.1.min.css");
    //Floating Notify (Buttons)
    //Html.AppendCssFileParts("~/Administration/Content/FloatNotify/FloatingNotify.css");

    int count = 0;
    int countRender = 0;
    //bool x =
    bool IsShowOrNot = EngineContext.Current.Resolve<Seagull.Core.IWorkContext>().CurrentUser.ShowDashboard;

    //page title
    ViewBag.Title = T("Admin.Dashboard").Text;
    var currentUser = EngineContext.Current.Resolve<Seagull.Core.IWorkContext>().CurrentUser;
    string EntityName = currentUser.EntityUserId.HasValue ? currentUser.EntityUserId.Value > 0 ? EngineContext.Current.Resolve<Seagull.Services.UserEntitys.IUserEntityService>().GetUserEntityById(currentUser.EntityUserId.Value).Name : string.Empty : string.Empty;
    ViewData["SelectedTab"] = null;
    var IsRtl = EngineContext.Current.ContainerManager.Resolve<Seagull.Core.IWorkContext>().WorkingLanguage.Rtl.ToString().ToLower();
    var IsAdmin = EngineContext.Current.Resolve<Seagull.Core.IWorkContext>().CurrentUser.UserRoles.Where(a => a.Name == "Administrators").Count() > 0;
    bool IsNotMailOperator = !(EngineContext.Current.Resolve<Seagull.Core.IWorkContext>().CurrentUser.UserRoles.Where(a => a.Name.Contains("Postal Operator")).Count() > 0);

}
